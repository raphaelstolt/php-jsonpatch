{
    "name": "mazubieta/php-jsonpatch",
    "description": "Implementation of JSON Patch (http://tools.ietf.org/html/rfc6902), forked from https://github.com/raphaelstolt/php-jsonpatch",
    "keywords": ["json", "json patch", "json modification"],
    "homepage": "https://github.com/mazubieta/php-jsonpatch",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Raphael Stolt",
            "email": "raphael.stolt@gmail.com",
            "homepage": "http://raphaelstolt.blogspot.com"
        },
        {
            "name": "Manuel Zubieta",
            "email": "mazubieta@gmail.com",
            "homepage": "http://mazubieta.com"
        }
    ],
    "support": {
        "issues": "https://github.com/mazubieta/php-jsonpatch/issues"
    },
    "require": {
        "php": ">=5.3.0",
        "php-jsonpointer/php-jsonpointer": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Rs\\Json": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
